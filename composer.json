{
  "name": "piccioni/london",
  "license": "MIT",
  "description": "Theme based on Bootstrap 5.",
  "type": "wordpress-theme",
  "minimum-stability": "stable",
  "authors": [
    {
      "email": "xlthlx@gmail.com",
      "name": "xlthlx"
    }
  ],
  "require": {
    "cmb2/cmb2": "^v2.10.1",
    "composer/installers": "^2.1.1",
    "johnbillion/extended-cpts": "^5.0.2",
    "twbs/bootstrap": "^v5.1.3"
  },
  "extra": {
    "installer-paths": {
      "inc/{$vendor}/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "require-dev": {
    "dxw/phar-install": "^1.1.2"
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/phar-install"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
